  "/events/:id": {
    "GET": {
      "description": "Returns a specific event by its ID.",
      "params": {
        "id": "Unique event identifier"
      },
      "responses": {
        "200": "Returns the event object",
        "404": "Not found if event ID is invalid"
      },
      "example_response": {
        "id": "67cb4183635459a37c4b",
        "name": "New Event",
        "createdAt": "2025-03-07T18:57:07.408+00:00",
